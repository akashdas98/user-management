{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "generate": {
    "service": {
      "spec": true
    },
    "controller": {
      "spec": true
    },
    "module": {
      "spec": false
    },
    "class": {
      "spec": false
    },
    "interface": {
      "spec": false
    },
    "pipe": {
      "spec": true
    },
    "filter": {
      "spec": true
    },
    "guard": {
      "spec": true
    },
    "middleware": {
      "spec": true
    }
  }
}
